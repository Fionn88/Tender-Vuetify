iner>\n    <b-card-group deck v-for=\"i in Math.ceil(certificates.length / 4)\" :key=\"i\">\n      <b-card\n        v-bind:key=\"certificate.id\"\n        v-for=\"certificate in certificates.slice((i - 1) * 4, (i - 1) * 4 + 4)\"\n        :title=\"certificate.tenderId\"\n        class=\"mb-2\"\n      >\n        <b-card-text>\n          No. \\n\n          {{ certificate.amount }}\n        </b-card-text>\n\n        <v-row align-v=\"center\">\n          <b-col sm>\n            <b-button variant=\"primary\" v-on:click=\"showDetail(certificate)\"\n              >View Certificate details</b-button\n            >\n          </b-col>\n        </v-row>\n      </b-card>\n      <v-expand-transition>\n        <v-card\n          v-if=\"reveal\"\n          class=\"transition-fast-in-fast-out v-card--reveal\"\n          style=\"height: 100%\"\n        >\n          <v-card-text class=\"pb-0\">\n            <p class=\"text-h4 text--primary\">憑證資訊</p>\n\n            <b-card-sub-title class=\"mb-2\"\n              >txid: {{ currentItem.txid }}</b-card-sub-title\n            >\n\n            <b-list-group flush>\n              <b-list-group-item\n                >標案號： {{ currentItem.tenderId }}</b-list-group-item\n              >\n              <b-list-group-item\n                >銀行帳戶代碼： {{ currentItem.accountCode }}</b-list-group-item\n              >\n              <b-list-group-item\n                >銀行帳戶： {{ currentItem.account }}</b-list-group-item\n              >\n              <b-list-group-item>\n                銀行戶名： {{ currentItem.name }}</b-list-group-item\n              >\n              <b-list-group-item>\n                銀行帳戶幣別： {{ currentItem.currency }}</b-list-group-item\n              >\n              <b-list-group-item>\n                銀行分行代號： {{ currentItem.branch }}</b-list-group-item\n              >\n              <b-list-group-item>\n                憑證金額： {{ currentItem.amount }}</b-list-group-item\n              >\n              <b-list-group-item>\n                憑證狀態： {{ currentItem.status }}</b-list-group-item\n              >\n            </b-list-group>\n          </v-card-text>\n          <v-card-actions class=\"pt-0\">\n            <v-btn text color=\"teal accent-4\" @click=\"reveal = false\">\n              Close\n            </v-btn>\n          </v-card-actions>\n        </v-card>\n      </v-expand-transition>\n    </b-card-group>\n  </b-container>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      certificates: [],\n      reveal: false,\n      currentItem: {\n        id: \"\",\n        accountCode: \"\",\n        account: \"\",\n        name: \"\",\n        currency: \"\",\n        branch: \"\",\n        amount: \"\",\n        tenderId: \"\",\n        status: \"\",\n        txid: \"\",\n      },\n    };\n  },\n  methods: {\n    showDetail(item) {\n      this.reveal = true;\n      this.currentItem = item;\n    },\n  },\n  mounted() {\n    axios\n      .get(\"http://127.0.0.1:3000/certificateRevoke\", {})\n      .then((resp) => {\n        console.log(resp);\n        this.certificates = resp.data.certificate;\n      })\n      .catch((err) => {\n        console.log(err);\n      });\n  },\n};\n</script>\n\n<style>\n</style>"]},"metadata":{},"sourceType":"module"}